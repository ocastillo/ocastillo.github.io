{
  "name": "orlandocastillo_com",
  "version": "1.0.0",
  "description": "Website for OrlandoCastillo.com",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node index.js",
    "start-dev": "nodemon --config nodemon.json",
    "dev": "npm-run-all --parallel build-css watch-js build-css watch-css start-dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p && node-sass app/client/src/scss -o app/client/public/css",
    "watch-js": "webpack -d --watch",
    "build-css": "node-sass app/client/src/scss -o app/client/public/css",
    "watch-css": "node-sass --watch app/client/src/scss -o app/client/public/css"
  },
  "author": "Orlando Castillo",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-flash": "0.0.2",
    "express-react-views": "^0.10.4",
    "express-session": "^1.15.6",
    "express-validator": "^4.2.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.0.0",
    "react-async-script": "^0.9.1",
    "react-dom": "^16.0.0",
    "react-google-recaptcha": "^0.11.0",
    "sanitize-html": "^1.18.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.8.1"
  }
}
